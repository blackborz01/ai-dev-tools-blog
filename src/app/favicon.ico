<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512">
  <defs>
    <!-- High-quality gradient with multiple stops -->
    <linearGradient id="premium" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00f5ff;stop-opacity:1" />
      <stop offset="25%" style="stop-color:#00d4ff;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#0ea5e9;stop-opacity:1" />
      <stop offset="75%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#6366f1;stop-opacity:1" />
    </linearGradient>
    
    <!-- Metallic shine effect -->
    <linearGradient id="shine" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#ffffff;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#ffffff;stop-opacity:0.1" />
    </linearGradient>
    
    <!-- Shadow filter for depth -->
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur in="SourceAlpha" stdDeviation="8"/>
      <feOffset dx="0" dy="8" result="offsetblur"/>
      <feFlood flood-color="#000000" flood-opacity="0.4"/>
      <feComposite in2="offsetblur" operator="in"/>
      <feMerge>
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    
    <!-- Glow effect -->
    <filter id="glow">
      <feGaussianBlur stdDeviation="6" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="512" height="512" rx="128" fill="url(#premium)"/>
  
  <!-- Inner shadow for depth -->
  <rect width="512" height="512" rx="128" fill="none" stroke="rgba(0,0,0,0.2)" stroke-width="2" opacity="0.5"/>
  
  <!-- Lightning bolt symbol for speed/boost -->
  <g transform="translate(256, 256)" filter="url(#shadow)">
    <!-- Main lightning bolt -->
    <path d="M -60 -140 L -20 -140 L -40 -20 L 20 -20 L -10 140 L -30 20 L -80 20 Z" 
          fill="white" 
          stroke="rgba(255,255,255,0.9)" 
          stroke-width="4"/>
    
    <!-- Inner highlight -->
    <path d="M -50 -120 L -25 -120 L -40 -30 L 10 -30 L -15 110 L -30 10 L -70 10 Z" 
          fill="url(#shine)" 
          opacity="0.6"/>
  </g>
  
  <!-- Speed lines for motion effect -->
  <g transform="translate(256, 256)" opacity="0.8">
    <rect x="-200" y="-60" width="80" height="8" rx="4" fill="white" opacity="0.6"/>
    <rect x="-210" y="-30" width="100" height="10" rx="5" fill="white" opacity="0.8"/>
    <rect x="-200" y="0" width="80" height="8" rx="4" fill="white" opacity="0.6"/>
    <rect x="-190" y="30" width="60" height="6" rx="3" fill="white" opacity="0.4"/>
    <rect x="-180" y="55" width="40" height="4" rx="2" fill="white" opacity="0.3"/>
  </g>
  
  <!-- Top shine for premium look -->
  <ellipse cx="256" cy="140" rx="180" ry="100" fill="white" opacity="0.15"/>
  
  <!-- Subtle border glow -->
  <rect width="512" height="512" rx="128" fill="none" stroke="white" stroke-width="2" opacity="0.3" filter="url(#glow)"/>
</svg>